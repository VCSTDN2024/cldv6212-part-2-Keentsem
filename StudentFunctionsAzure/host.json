{
  "version": "2.0",
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      },
      "enableLiveMetricsFilters": true
    }
  },
  "extensions": {
    "http": {
      "routePrefix": "api"
    }
  },
  "cors": {
    "allowedOrigins": [
      "http://localhost:5231",
      "https://localhost:7235",
      "http://localhost:5000",
      "https://localhost:5001"
    ],
    "supportCredentials": false
  }
}
